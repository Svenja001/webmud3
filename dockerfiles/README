

#   im .. folgendes ausfuehren (ggf Version anpassen)
docker build -f dockerfiles/ng.dockerfile -t myonara/webmud3:v0.0.10 .

#   test als standalone container
docker run -d -p 5000:5000 --name webmud3test myonara/webmud3:v0.0.10

#   ebenso nach einem swarm init:
docker stack deploy -c dockerfiles/w3_docker_compose_secret.yml webmud3a

#deinstallieren:
docker service rm webmud3a_web

#service anschauen:
docker service ls

#service details:
docker service ps --no-trunc webmud3a_web

# oder (ohgott):
journalctl -u docker


